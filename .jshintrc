{
    // Enforcing options
    "eqeqeq": false,
    "curly": true,
    "freeze": true,
    "futurehostile": true,
    "indent": 4,
    "latedef": "nofunc",
    "noarg": true,
    "noempty": true,
    "shadow": false,
    "undef": true,
    "unused": true,

    // Relaxing options
    "onevar": false,
    "asi": false,
    "eqnull": false,
    "expr": false,
    "loopfunc": true,
    "sub": true,
    
    // Environment
    "node": true
}
