{
  "name": "ziti",
  "version": "0.0.2",
  "description": "MySQL fast ORM",
  "main": "index.js",
  "scripts": {
    "test:lint": "jshint",
    "test:unit": "mocha",
    "test": "npm run test:lint && npm run test:unit",
    "docs:ziti": "jsdoc2md index.js lib/db.js > docs/api/ziti.md",
    "docs:model": "jsdoc2md lib/model.js > docs/api/model.md",
    "docs:instance": "jsdoc2md lib/model-instance.js > docs/api/instance.md",
    "docs:types": "jsdoc2md lib/types.js lib/type/*.js > docs/api/types.md",
    "docs:functions": "jsdoc2md lib/functions.js > docs/api/functions.md",
    "docs": "npm run docs:ziti && npm run docs:model && npm run docs:instance && npm run docs:types && npm run docs:functions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaoner/ziti"
  },
  "keywords": [
    "MySQL",
    "ORM"
  ],
  "author": "Alexandre Laurent",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/shaoner/ziti/issues"
  },
  "homepage": "https://github.com/shaoner/ziti",
  "dependencies": {
    "bluebird": "^2.9.25",
    "lodash": "^3.9.1",
    "mysql": "^2.6.2"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "jsdoc-to-markdown": "^1.1.1",
    "jshint": "^2.7.0",
    "mocha": "^2.2.5"
  }
}
